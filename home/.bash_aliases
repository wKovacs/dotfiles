alias sleep-displays="sleep 0.5 && qdbus org.kde.kglobalaccel /component/org_kde_powerdevil invokeShortcut 'Turn Off Screen'"
alias vi="/usr/bin/nvim"
alias vim="/usr/bin/nvim"
alias sudo="sudo "
alias sudoe="sudo -E "
alias remmina="systemctl --user start remmina-autostart.service"
alias xfreerdp="xfreerdp3"

alias ls='ls --color -F'
